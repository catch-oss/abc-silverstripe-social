{
    "name": "azt3k/abc-silverstripe-social",
    "description": "Library that adds some social media functionality to Silverstripe",
    "license": "BSD-3-Clause-Clear",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Aaron Latham-Ilari",
            "email": "abcdigital@abcdigital.co.nz"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gl.catchdesign.co.nz:catch/abc-silverstripe-taggable.git"
        },
        {
            "type": "vcs",
            "url": "git@gl.catchdesign.co.nz:catch/abc-sliverstripe.git"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "composer/installers": ">=1.0",
        "silverstripe/framework": "^4@stable",
        "silverstripe/cms": "^4@stable",
        "silverstripe/crontask": "^2.1",
        "azt3k/abc-silverstripe": "dev-master",
        "azt3k/abc-silverstripe-taggable": "^4",
        "themattharris/tmhoauth": "dev-master",
        "janu-software/facebook-php-sdk": "0.1.*@stable",
        "cosenary/instagram": "2.3.*@stable",
        "guzzlehttp/guzzle": "*@stable",
        "composer-fallback/php-http.client-implementation.guzzle7": "*@stable"
    },
    "autoload": {
        "psr-4": {
            "Azt3k\\SS\\Social\\": "code/"
        }
    },
    "extra": {
        "expose": [
            "css","img","js"
        ]
    }
}
