{
  "name": "azt3k/abc-silverstripe-social",
  "description": "Library that adds some social media functionality to Silverstripe",
  "license": "BSD-3-Clause-Clear",
  "type": "silverstripe-vendormodule",
  "authors": [
    {
      "name": "Aaron Latham-Ilari",
      "email": "abcdigital@abcdigital.co.nz"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:catch-oss/tmhOAuth.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:catch-oss/abc-silverstripe-taggable.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:catch-oss/abc-silverstripe.git"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "silverstripe/framework": "^5",
    "silverstripe/cms": "^5",
    "silverstripe/crontask": "^3.0",
    "composer/installers": "^2.2",
    "azt3k/abc-silverstripe": "dev-feature/ss5-update",
    "azt3k/abc-silverstripe-taggable": "dev-feature/ss5-update",
    "themattharris/tmhoauth": "dev-feature/ss5-update",
    "janu-software/facebook-php-sdk": "0.2.*@stable",
    "cosenary/instagram": "2.3.*@stable",
    "guzzlehttp/guzzle": ">=7.4.5",
    "composer-fallback/php-http.client-implementation.guzzle7": "*@stable"
  },
  "autoload": {
    "psr-4": {
      "Azt3k\\SS\\Social\\": "code/"
    }
  },
  "extra": {
    "expose": [
      "css",
      "img",
      "js"
    ]
  }
}
